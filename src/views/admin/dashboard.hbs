<div class="col-md-12 col-sm-12 col-xs-12">
    
    <div class="x_panel">
        <div class="x_title">
        <h2>All Branch</h2>
        <ul class="nav navbar-right panel_toolbox">
            <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
            <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
            <ul class="dropdown-menu" role="menu">
                <li><a href="#">Settings 1</a>
                </li>
                <li><a href="#">Settings 2</a>
                </li>
            </ul>
            </li>
            <li><a class="close-link"><i class="fa fa-close"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
        </div>
        <div class="x_content">

        <table id="datatable" class="table table-striped table-bordered">
            <thead>
            <tr>
                <th>N/S</th>
                <th>Name</th>
                <th>E-mail</th>
                <th>Phone</th>
                <th>Address</th>
                <th>Actions</th>
            </tr>
            </thead>


            <tbody>
                {{#each branches}}
                <tr>
                    <td>{{inc @index}}</td>
                    <td id="name{{_id}}">{{name}}</td>
                    <td id="email{{_id}}">{{email}}</td>
                    <td id="phone{{_id}}">{{phone}}</td>
                    <td id="address{{_id}}">{{address}}
                         
                    <td>
                        {{!-- <a href="#" class="btn btn-primary btn-sm edit"><i class="fa fa-edit"></i></a> --}}
                        {{!-- <button data-toggle="modal" data-id="{{_id}}" class="open-editBranch btn btn-primary btn-sm"><i class="fa fa-edit"></i></button> --}}
                        {{!-- <button class="btn btn-primary btn-sm" onclick="editbranchModal(this.value)" value="{{_id}}" data-toggle="modal" data-target="#editBranch"><i class="fa fa-edit"></i></button> --}}



                        <button data-toggle="modal" branchId="{{_id}}" name="{{name}}" email="{{email}}" phone="{{phone}}" address="{{address}}" country="{{country}}" state="{{state}}" city="{{city}}" data-target="#editBranch" class="btn btn-primary btn-sm">
                            <i class="fa fa-edit"></i>
                        </button>
                        <a href="/branch/view/{{_id}}" class="btn btn-success btn-sm"><i class="fa fa-eye"></i></a> 
                        <button onclick="deleteBranch(this.value)" value="{{_id}}" class="btn btn-danger btn-sm"><i class="fa fa-trash"></i></button>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        </div>
    </div>
</div>

</div>


